{
  "root": true,
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": [
    "flowtype"
  ],
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jquery": true,
    "jest": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-use-before-define": "off"
  }
}
