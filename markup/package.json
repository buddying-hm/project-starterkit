{
  "private": true,
  "name": "ecss-template",
  "version": "1.0.0",
  "output": {
    "markup": {
      "context": "../public",
      "css": "assets/css",
      "js": "assets/js",
      "img": "assets/img",
      "font": "assets/font"
    },
    "server": {
      "context": "../source/html",
      "css": "assets/css",
      "js": "assets/js",
      "img": "assets/img",
      "font": "assets/font"
    }
  },
  "scripts": {
    "start": "npm run watch",
    "test": "jest",
    "lint": "npm run eslint",
    "build": "npm run cleaning:s && node tasks/build.js",
    "watch": "npm run cleaning && node tasks/watch.js markup",
    "watch:s": "npm run cleaning:s && node tasks/watch.js server",
    "compile": "npm run cleaning && node tasks/compile.js markup",
    "compile:s": "npm run cleaning:s && node tasks/compile.js server",
    "cleaning": "node tasks/clean.js",
    "cleaning:s": "node tasks/clean.js server"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.0",
    "browser-sync": "^2.18.8",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "ejs": "^2.5.6",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-eslint": "^4.0.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "gulp-html-beautify": "^1.0.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "p-path": "file:tasks/lib/p-path",
    "postcss-loader": "^2.0.8",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1"
  },
  "dependencies": {
    "jquery": "^3.2.1"
  },
  "jest": {
    "rootDir": "view"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
