    "babel-preset-es2015": "^6.24.0",
    "browser-sync": "^2.18.8",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "del": "^2.2.2",
    "ejs": "^2.5.6",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-eslint": "^4.0.0",
    "eslint-plugin-import": "^2.7.0",
