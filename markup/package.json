      "css": "assets/css",
      "js": "assets/js",
      "img": "assets/img"
    },
    "server": {
      "context": "../source/html",
      "css": "assets/css",
      "js": "assets/js",
      "img": "assets/img"
    }
  },
  "scripts": {
    "start": "npm run watch",
    "test": "jest",
